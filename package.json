{
  "name": "ck-signal-table",
  "version": "1.0.0",
  "description": "A CanKing extension showing CAN signal values in a table",
  "author": "Krister Johansson",
  "canking": {
    "workspaceViewName": "Signal Data Table",
    "types": [
      "WorkspaceView"
    ]
  },
  "private": true,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm-run-all --parallel dev canking",
    "startpreview": "npm-run-all --parallel preview canking",
    "help": "start ./node_modules/@kvaser/canking-api/doc/index.html",
    "dev": "vite --port 4000",
    "preview": "vite preview --port 4000",
    "canking": "cross-env \"C:/Program Files/Kvaser CanKing/Kvaser CanKing.exe\" --ck-debug-pkg \"./package.json\" --ck-debug-port 4000",
    "build": "tsc -b && vite build",
    "prepack": "npm run build",
    "prebuild": "rimraf dist"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@kvaser/canking-api": "^7.3.0",
    "@mui/material": "^6.1.3",
    "fast-deep-equal": "^3.1.3",
    "google-protobuf": "^3.21.4",
    "long": "^5.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^5.4.9"
  }
}